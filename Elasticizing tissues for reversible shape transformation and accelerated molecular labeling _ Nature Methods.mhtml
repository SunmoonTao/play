*, ::after, ::before { box-sizing: border-box; outline: none; }

html { height: 100%; font-size: 18px; font-family: Lato, Helvetica, Arial, =
sans-serif; text-size-adjust: 100%; -webkit-tap-highlight-color: rgba(0, 0,=
 0, 0); }

@media only screen and (max-width: 767px) {
  html { font-size: 16px; }
}

html, body { margin: 0px; height: 100%; overflow: hidden; }

body { min-height: 100vh; height: 100%; font-family: Lato, Helvetica, Arial=
, sans-serif; font-size: 100%; font-weight: 400; line-height: 1.5; color: r=
gb(74, 73, 74); text-align: left; background-color: transparent; }

.overlay__status-head { margin: 0px; }

.overlay__popup { bottom: 15px; width: auto; min-width: 200px; height: auto=
; padding: 0.8rem; color: rgb(255, 255, 255); background-color: rgb(48, 55,=
 61); border-radius: 4px; font-size: 0.8rem; }

.overlay__content::after { content: ""; display: block; position: absolute;=
 left: 15px; height: 0px; border-top: 15px solid rgb(48, 55, 61); border-le=
ft: 15px solid transparent; border-right: 15px solid transparent; }

.overlay__status { list-style-type: none; padding-left: 0px; }

.toggle-content { display: none; height: 0px; opacity: 0; overflow: hidden;=
 transition: all 2s ease-in-out 0s; pointer-events: unset; }

.toggle-content.is-visible { display: block; height: auto; opacity: 1; tran=
sition: all 2s ease-in-out 0s; pointer-events: unset; }

.overlay__status-item { position: relative; line-height: 1; margin-bottom: =
0.5rem; padding-left: 1.5em; }

.overlay__status-item::before { content: ""; display: none; position: absol=
ute; left: 0px; top: 0px; width: 1em; height: 1em; background-size: contain=
; }

.overlay__status-item--searching::before { display: block; background-image=
: url("../icons/kopernio-spinner.svg"); animation: 1.5s linear 0s infinite =
normal none running spin; }

.overlay__status-item--success { color: rgb(204, 204, 204); }

.overlay__status-item--success.main-source { color: rgb(255, 255, 255); }

.overlay__status-item--fail { color: rgb(204, 204, 204); }

.overlay__status-item--success.main-source::before { display: block; backgr=
ound-image: url("../icons/kopernio-success.svg"); }

.overlay__status-item--success::before { display: block; background-image: =
url("../icons/kopernio-success-secondary.svg"); }

.overlay__status-item--fail::before { display: block; background-image: url=
("../icons/kopernio-fail.svg"); }

.non-logged-in { overflow: hidden; height: 70px; visibility: hidden; }

@keyframes spin {=20
  100% { transform: rotate(360deg); }
